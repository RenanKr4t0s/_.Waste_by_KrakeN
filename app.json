{
  "expo": {
    "name": "!Waste",
    "slug": "!Waste",
    "version": "0.0.2",
    "orientation": "portrait",
    "scheme":"!Waste",
    "icon": "./assets/octopus_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/octopus_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/octopus_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/octopus_icon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
